{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "jobs/**.md",
          "jobs/**.yml",
          "relationships/**.md",
          "relationships/**.yml",
          "equipment/**.md",
          "equipment/**.yml",
          "products/**.md",
          "products/**.yml",
          "tasks/**.md",
          "tasks/**.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "Monica",
      "_appFaviconPath": "favicon.ico",
      "_appLogoPath": "images/MonicaLogo.png",
      "_appFooter": "Copyright &copy; 2022 Oliver Waits",
      "_enableSearch": "true",
      "_gitContribute": {
            "repo": "https://github.com/owaits/Monica.Help",
            "branch": "master"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}